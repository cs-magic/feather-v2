    "@use-gesture/react": "^10.3.0",
    "bufferutil": "^4.0.7",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "immer": "^10.0.2",
    "lucide-react": "0.105.0-alpha.4",
    "next": "13.4.12",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-joystick": "^1.0.3",
    "react-joystick-component": "^6.2.1",
    "react-toastify": "^9.1.3",
